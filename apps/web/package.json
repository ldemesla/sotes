{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:start": "docker-compose --env-file .env -f ./src/server/docker-compose.yaml up -d",
    "db:migrate:make": "pnpm kysely migrate:make",
    "db:migrate:up": "pnpm kysely migrate:up && pnpm db:typegen",
    "db:migrate": "tsx ./src/scripts/create-qdrant-collections.ts && pnpm kysely migrate:latest && pnpm db:typegen",
    "db:migrate:down": "pnpm kysely migrate:down",
    "seed:run": "pnpm kysely seed:run",
    "db:typegen": "pnpm kysely-codegen --out-file ./../../packages/shared/src/types/database.types.ts",
    "dev": "tsx ./src/scripts/check-env.ts && concurrently \"pnpm db:start\" \"next dev --turbo\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest"
  },
  "dependencies": {
    "@openai/realtime-api-beta": "github:openai/openai-realtime-api-beta",
    "@qdrant/js-client-rest": "^1.12.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tiptap/core": "2.8.0",
    "@tiptap/extension-heading": "2.8.0",
    "@tiptap/pm": "2.8.0",
    "@tiptap/react": "2.8.0",
    "@tiptap/starter-kit": "2.8.0",
    "bullmq": "^5.21.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "debounce": "^2.2.0",
    "ioredis": "^5.4.1",
    "kysely": "^0.27.4",
    "next": "15.0.0",
    "next-auth": "^4.24.8",
    "openai": "^4.68.2",
    "pg": "^8.13.0",
    "postgres-array": "^3.0.2",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020",
    "shared": "*",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tiptap-markdown": "^0.8.10",
    "utils": "*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "kysely-codegen": "^0.17.0",
    "kysely-ctl": "^0.9.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^2.1.3"
  }
}
