{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:start": "docker-compose --env-file .env -f ./src/server/docker-compose.yaml up -d",
    "db:migrate:make": "pnpm kysely migrate:make",
    "db:migrate:up": "pnpm kysely migrate:up && pnpm db:typegen",
    "db:migrate": "pnpm kysely migrate:latest && pnpm db:typegen",
    "db:migrate:down": "pnpm kysely migrate:down",
    "db:typegen": "pnpm kysely-codegen --out-file ./src/server/db/database.types.ts",
    "dev": "concurrently \"db:start\" \"next dev --turbo\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@openai/realtime-api-beta": "github:openai/openai-realtime-api-beta",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "kysely": "^0.27.4",
    "next": "15.0.0",
    "pg": "^8.13.0",
    "react": "19.0.0-rc-65a56d0e-20241020",
    "react-dom": "19.0.0-rc-65a56d0e-20241020",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.0",
    "kysely-codegen": "^0.17.0",
    "kysely-ctl": "^0.9.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
